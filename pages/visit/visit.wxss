/* pages/visit/visit.wxss */
page{
  width: 100%;
  height: 100%;
    background: url('http://ylbb-business.oss-cn-beijing.aliyuncs.com/visit454534534.png') no-repeat #F5F5F5;
  background-size:750rpx 360rpx; 
}
.content{
  width: 100%;
  height: 100%;

}
.mains{
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}

swiper{
  width: 100%;
  min-height: 90%;
}
swiper-item{
  height: 100%;

}
.changeNav{
  width: 588rpx;
  height: 84rpx;
  line-height: 84rpx;
  margin: 0 auto;
  background: #fff;
  padding: 0 81rpx;
  border-top: solid 2rpx #F5F5F5;
}
.changeNav view{
  width: 112rpx;
  padding: 0 42rpx;
  float: left;
  font-size:28rpx; 
  color:#333333;

}
.changeNav view text{
  width: 112rpx;
  float: left;
  font-size:28rpx; 
  color:#333333;

}
.changeNav view.cli text{
  color:#5EA3FE;
  border-bottom: solid 4rpx #5EA3FE;
}
.header{
  width: 100%;
  height: 242rpx;
  padding-top: 30rpx;
  
}
.visit_top{
  width: 670rpx;
  height: 212rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  background: url('http://ylbb-business.oss-cn-beijing.aliyuncs.com/icon-ylbaby15454564.png') no-repeat 570rpx 150rpx #5EA3FE;
  background-size:104rpx 78rpx; 
}
.visit_tit{
  width: 160rpx;
  height:56rpx;
  color: #fff;
  float: right;
  font-size: 40rpx;
  margin: 24rpx 32rpx;
}
.visit_wait,.visit_solve{
  width: 50%;
  float: left;
  color: #fff;
  margin-top: 52rpx;
  font-size: 28rpx;
  margin-right: 10rpx;
  margin-left: 24rpx;
}
.visit_solve{
  margin-top: 0;
}
.visit_wait text,.visit_solve text{
  font-size: 48rpx;
  padding-left: 16rpx;
}
.visit_list{
  width: 670rpx;
  margin: 0 auto;
}
.visit_list{
  height:70%;
  overflow: scroll;
  -webkit-overflow-scrolling: touch;
}
.list_main{
  width: 100%;
  height: 124rpx;
  background: #fff;
  border-radius:10rpx;
  margin-top: 30rpx;  
}
.list_mains{
  height: 162rpx;
    width: 100%;
  background: #fff;
  border-radius:10rpx;
  margin-top: 30rpx;  
}
.visit_left,.visit_right{
  width: 50%;
  float: left;
}
.visit_lefts{
    width: 70%;
  float: left;
}
.visit_rights{
    width: 30%;
  float: left;
}
.visitName{
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;  
  float: left;
  font-size: 32rpx;
  color: #333;
  margin-top: 22rpx;
  text-indent: 32rpx;
}
.visitNames{
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;  
  float: left;
  font-size: 32rpx;
  color: #333;
  margin-top: 22rpx;
  text-indent: 32rpx;
}
.usericon{
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin: 40rpx 32rpx;
}
.usericon image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.usertext{
  float: left;
  width: 250rpx;
}

.visitPhone,.visitstatus{
   width: 100%;
  height: 34rpx;
  line-height: 34rpx;   
  float: left;
  font-size: 24rpx;
  color: #999;
  margin-top: 2rpx;
  text-indent: 32rpx; 
}

.visitPhones{
   width: 100%;
  height: 34rpx;
  line-height: 34rpx;   
  float: left;
  font-size: 24rpx;
  color: #999;
  margin-top: 2rpx;
  text-indent: 32rpx; 
}

.visitIcon{
  width: 56rpx;
  height: 56rpx;
  float: right;
   margin: 38rpx 34rpx;
   
}
.visitIcons{
   margin: 54rpx 34rpx;
   
}
.visitIcon image{
  width: 100%;
  height: 100%;
 
}
